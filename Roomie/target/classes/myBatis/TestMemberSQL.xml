<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
	
	<mapper namespace="testMember">
	
		<!-- 채팅 내용 리스트 -->
		<select id="selectMemberId" parameterType="hashmap" resultType="hashmap">
			<![CDATA[
				SELECT
				*
				FROM
					MEMBER
				WHERE
					MEM_ID = #{MEM_ID}
			]]>
		</select>
		
	</mapper>